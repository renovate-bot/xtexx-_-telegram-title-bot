{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"version": 2,
	"public": true,
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "api/index.js"
		}
	],
	"redirects": [
		{
			"source": "/",
			"destination": "/api",
			"permanent": false
		}
	],
	"functions": {
		"api/index.js": {
			"maxDuration": 20
		}
	}
}